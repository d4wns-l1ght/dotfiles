# theme
include ~/.config/newsboat/dark

browser firefox
# bindings
unbind-key h
unbind-key j
unbind-key k
unbind-key l
unbind-key g
unbind-key G

bind-key h quit
bind-key j down
bind-key k up
bind-key l open
bind-key g home
bind-key G end

ignore-article "https://www.youtube.com/feeds/videos.xml?channel_id=UCGmO0S4S-AunjRdmxA6TQYg" "title =~ \"#DECO*27\""
ignore-article "https://www.youtube.com/feeds/videos.xml?channel_id=UCGteuPMOJgrJ1ZiVmtfqwFA" "title =~ \"#\""
